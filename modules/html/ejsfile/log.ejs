<script src="https://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>

<% if (isfile) { %>
<div id="title">
    일자 : <%-day%><br/>아이디 : <%-userid%><br/>
    <input type="button" name="reset" value="처음으로" id="goback" onclick="window.location='/';"/>
</div>
<div id="file">
<% } else { %>
<div id="folder">
<div id="title">
    로그파일을 선택해주세요.<br/>
    <input type="button" name="reset" value="처음으로" id="goback" onclick="window.location='/';"/>
</div>
<% } %>
    <%-log%>
</div>

<style>
    #title {
        margin-top: 50px;
        text-align: center;
        font-size: 30px;
        font-weight: 500;
    }
    #folder {
        margin-top: 20px;
        text-align: center;
    }
    #file {
        font-size: 10px;
        margin-top: 40px;
        margin-bottom: 30px;
        margin-left: 3%;
        margin-right: 3%;
    }
    input {
        margin-top: 10px;
        font-size: 25px;
        padding: 5px 10px;
    }
    #goback {
        margin-top: 15px;
        margin-bottom: 20px;
        font-size: 17.5px;
    }
</style>